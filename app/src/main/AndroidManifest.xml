        </activity>
        <activity android:name=".CircleViewActivity"/>
        <activity android:name=".CustomView2Activity"/>
        <activity android:name=".CustomView3Activity" android:theme="@style/Theme_CustomView3Activity"/>
        <activity android:name=".CustomView4Activity"/>
    </application>

</manifest>